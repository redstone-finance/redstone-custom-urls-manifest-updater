{
  "name": "redstone-custom-url-manifest-updater-frontend",
  "version": "0.0.1",
  "description": "Redstone custom URL manifest frontend",
  "license": "MIT",
  "dependencies": {
    "@monaco-editor/react": "^4.4.6",
    "axios": "^1.2.3",
    "ethers": "^5.7.2",
    "jsonpath": "^1.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.2",
    "react-router-dom": "6.6.2"
  },
  "devDependencies": {
    "@types/jsonpath": "^0.2.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/node": "^18.11.18",
    "buffer": "^6.0.3",
    "parcel": "^2.8.2",
    "postcss": "^8.4.12",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4"
  },
  "scripts": {
    "dev": "rm -rf .parcel-cache && parcel index.html",
    "build": "rm -rf dist && parcel build index.html"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "alias": {
    "process": false
  },
  "browser": {
    "crypto": false,
    "constants": false
  }
